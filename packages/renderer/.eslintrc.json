{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": [
    /** @see https://eslint.vuejs.org/rules/ */
    "plugin:vue/vue3-recommended",
    "plugin:prettier-vue/recommended"
  ],
  "ignorePatterns": ["**/*.ts", "**/*.tsx"],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 12,
    "sourceType": "module"
  }
}
